package sintaxe_variaveis_e_fluxo;

public class testa_caracteres {

	public static void main(String[] args) {
		char letra = 'a';
System.out.println(letra);

char valor = 65;
System.out.println(valor);

valor =(char)(valor + 1);
System.out.println(valor);

String palavra ="alura";
System.out.println(palavra);


	}
	
}
